    a1 := Array<Int,3>( 1, 2, 3 );
    a2 := Array<Int>( 1, 2, 3 );
    a3 := Array<>( 1, 2, 3 );
    a4 := Array( 1, 2, 3 );
    a5 := [3]Int( 1, 2, 3 );
    a6 := []Int( 1, 2, 3 );
    a7 := [ Int(1), 2, 3 ];

}



