
for_loops( i : UInt )->Int {
    strA := c"foobar";
    strB := c"macintosh";

    for chA, iterA in strA,
        chB, iterB in strB {
        tmp := [ chA, ' ', chB, 0 ];
        tx.c.puts( &tmp[0] );
        if chA == 'b':
            iterA.next();
    }
    return 0;

